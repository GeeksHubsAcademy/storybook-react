{
  "name": "storybook-react-geekshubs",
  "version": "0.0.8",
  "type": "module",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "createIndex": "create-index-plus ./src -r --extensions js jsx ",
    "patch": "npm version patch",
    "postpatch": "npm run createIndex",
    "prebuild": "npm run patch",
    "build": "npm run build:lib && npm run build:docs",
    "build:docs": "build-storybook -o docs",
    "build:lib": "rimraf dist && NODE_ENV=production babel src --out-dir dist",
    "predeploy": "npm run build",
    "deploy": "git add . && git commit -m 'build' && git push",
    "postdeploy": "npm run publi",
    "publi": "npm publish",
    "prepubli": "npm login"
  },
  "keywords": [
    "react",
    "components",
    "webComponents",
    "storybook"
  ],
  "author": "jupegarnica",
  "license": "ISC",
  "dependencies": {
    "react": "^16.10.2",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.2.3",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-backgrounds": "^5.2.3",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.3",
    "@storybook/addon-viewport": "^5.2.3",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "create-index-plus": "^1.1.1",
    "rimraf": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
